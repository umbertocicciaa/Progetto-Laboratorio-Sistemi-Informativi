<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="168.0" prefWidth="298.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.acquisti.ordine.UpdateOrdineController">
    <GridPane prefHeight="168.0" prefWidth="298.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="163.0" minWidth="10.0" prefWidth="152.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="146.0" minWidth="10.0" prefWidth="146.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button mnemonicParsing="false" onAction="#okButton" prefHeight="0.0" prefWidth="152.0" text="Ok"
                GridPane.rowIndex="3"/>
        <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelButton" prefHeight="26.0" prefWidth="146.0"
                text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label text="Stato"/>
        <Label text="Data" GridPane.rowIndex="1"/>
        <Label text="Quantita" GridPane.rowIndex="2"/>
        <TextField fx:id="statoField" GridPane.columnIndex="1"/>
        <TextField fx:id="quantitaField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <DatePicker fx:id="dataField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</AnchorPane>
