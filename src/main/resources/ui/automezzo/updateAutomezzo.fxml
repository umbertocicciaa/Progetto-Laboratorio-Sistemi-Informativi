<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane prefHeight="120.0" prefWidth="359.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.acquisti.automezzo.UpdateAutomezzoController">
    <header>
        <GridPane gridLinesVisible="true">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="177.0" minWidth="10.0" prefWidth="148.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="185.0" minWidth="10.0" prefWidth="145.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextArea fx:id="marcaField" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
                <TextArea fx:id="assicurazioneField" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextArea fx:id="prezzoField" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Label prefWidth="91.0" text="Marca" textAlignment="CENTER">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label prefHeight="18.0" prefWidth="157.0" text="Assicurazione" textAlignment="CENTER" GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label prefHeight="18.0" prefWidth="89.0" text="Prezzo" textAlignment="CENTER" GridPane.rowIndex="2">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleOkButton" prefHeight="26.0" prefWidth="175.0" text="Ok" GridPane.rowIndex="3" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButton" prefHeight="26.0" prefWidth="190.0" text="Cancella" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            </children>
        </GridPane>
    </header>
</DialogPane>
